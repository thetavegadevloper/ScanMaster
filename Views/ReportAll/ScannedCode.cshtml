@model IEnumerable<ScanMaster.Report>



  

    

    <div class="panel panel-default">
        <div class="panel-heading">Scanned Count</div>
        <div class="panel-body">

            <table id="myTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.ModelCode)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.TStamp)
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model.Select(m => new { m.TStamp, m.ModelCode }).Distinct())
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.ModelCode)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.TStamp)
                        </td>

                    </tr>
                }

            </table>


        </div>
    </div>
